{
  "name": "zai-test-service",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --env=development",
    "build-dev": "webpack --env=production",
    "start": "nodemon --watch src ./index",

    "dockerize": "npm run kill ^& npm run docker:build ^& npm run docker:start",
    "dockerize-dev": "npm run build-dev ^& npm run kill ^& npm run docker:build ^& npm run docker:start",
    "docker:build": "docker build -t zai-test-server:18.14.0 .",
    "docker:start": "docker run --network=zai-test -d -p 8000:8000 --name zailab zai-test-server:18.14.0",
    "kill": "docker stop zailab & docker rm zailab",
    "prune": "docker container prune",
    "restart": "docker restart zailab"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongodb": "^6.0.0",
    "mongoose": "^7.5.2",
    "morgan": "~1.9.1",
    "node-jsencrypt": "^1.0.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "testcafe": "^3.1.0",
    "testcafe-browser-provider-browserstack": "^1.15.1",
    "util": "^0.12.5",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "ts-loader": "^9.4.4",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
